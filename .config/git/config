[alias]
        aliases = !git config --get-regexp alias | sed 's/^alias.//g' | sed 's/ / = /1'
        br = branch
        ci = commit
        co = checkout
        st = status
        sw = switch
        #log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
[core]
        autocrlf = false
        ignorecase = false
        quotepath = false
        safecrlf = false
[pull]
        ff = only
        rebase = true
[merge]
        ff = false
        tool = vimdiff
        conflictStyle = zdiff3
[push]
        default = current
[grep]
        lineNumber = true
[commit]
        gpgsign = true
